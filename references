@INBOOK{Kubba2017-ph,
  title     = {Chapter Three - The Green Design and Construction Process},
  author    = {Kubba, Sam},
  editor    = {Kubba, Sam},
  booktitle = {Handbook of Green Building Design and Construction (Second
               Edition)},
  publisher = {Butterworth-Heinemann},
