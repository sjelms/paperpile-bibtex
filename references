@INBOOK{Warhurst2017-dx,
  title     = {A New Social Construction of Skill},
  author    = {Warhurst, Chris and Tilly, Chris and Gatta, Mary},
  editor    = {Warhurst, Chris and Mayhew, Ken and Finegold, David and Buchanan,
               John},
  booktitle = {The Oxford Handbook of Skills and Training},
  publisher = {Oxford University Press},
  pages     = {72--91},
  date      = {2017},
  isbn      = {9780199655366},
  abstract  = {There are a number of theoretical positions that inform analyses
               of skill. One such position is the social construction of skill.
               When it was first proposed it was driven by feminist concerns
               about the sex-typing of jobs and women’s exclusion from jobs
               labelled as skilled. This chapter offers a new social
               construction of skill. It appreciates that the old social
               construction of skill has not disappeared but points out that the
               context within which this construction occurred has changed, with
               weaker labour unions and the decline in the manufacturing
               industries. With more service jobs and stronger employers, the
               chapter argues that in the wealthier countries there have been
               two shifts: a shift in how skill has been defined and a shift in
               who has the power to define it. Focusing on gender, race and
               class, the chapters explains how the social construction of skill
               has been restructured in three ways. First, more importance is
               attached to ascription of skill. Second, who is and isn’t deemed
               to be skilled has changed. Third, the lines between achieved and
               ascribed skill are increasingly blurred. The chapter finishes by
               suggesting ways in which the discrimination arising from this new
               social construction of skill might be addressed.},
  url       = {https://academic.oup.com/edited-volume/27995/chapter/211706311},
  keywords  = {Oxford Handbook of Skills and Training;3\_Upgrade OCT
               2023;\_BibTex to MD Git;EPubs Books;Cross Ref Chapter;ePub;\_In
               Notion;\_In Readwise;VET;Workplace Learning;\_New Literature}
}

