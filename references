@ARTICLE{Gagg2014-gf,
  title    = "Cement and concrete as an engineering material: An historic
              appraisal and case study analysis",
  author   = "Gagg, Colin R",
  abstract = "Today, second only to water, concrete is the most consumed
              material, with three tonnes per year used for every person in the
              world. Twice as much concrete is used in construction as all
              other building materials combined. There is little doubt that
              concrete will remain in use as a construction material well into
              the future. However, with such extensive use of the material,
              discovery of any shortcoming or problem associated with concrete
              or reinforced concrete structures will become a matter of
              considerable public concern -- both from a safety perspective and
              associated costs of rectification. Accordingly, this paper will
              initially review the historic development of cements and concrete
              and will then focus on the mechanical response of concrete and
              reinforced concrete to its working environment. At appropriate
              points within the narrative, case study input will be used to
              illustrate or highlight principal themes.",
  journal  = "Engineering failure analysis",
  volume   =  40,
  pages    = "114--140",
  month    =  may,
  year     =  2014,
  url      = "https://www.sciencedirect.com/science/article/pii/S1350630714000387",
  keywords = "Cement and concrete; Historic development; Mechanical response;
              Structural failures; Failure case
              studies;AEC;+Notion;+Readwise;§New Literature;Construction
              4.0;*Upgrade Report May 2023;Journal Articles",
  issn     = "1350-6307",
  doi      = "10.1016/j.engfailanal.2014.02.004"
}

