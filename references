@BOOK{Mayring2014-xu,
  title     = "Qualitative content analysis: theoretical foundation, basic
               procedures and software solution",
  author    = "Mayring, Philipp",
  publisher = "AUT",
  pages     =  143,
  abstract  = "Qualitative Content Analysis designates a bundle of text analysis
               procedures integrating qualitative and quantitative steps of
               analysis, which makes it an approach of mixed methods. This
               contribution defines it with a background of quantitative content
               analysis and compares it with other social science text analysis
               approaches (e.g. Grounded Theory). The basic theoretical and
               methodological assumptions are elaborated: reference to a
               communication model, rule orientation of analysis, theoretical
               background of those content analytical rules, categories in the
               center of the procedure, necessity of pilot testing of categories
               and rules, necessity of intra- and inter-coder reliability
               checks. Then the two main procedures, inductive category
               formation and deductive category assignment, are described by
               step models. Finally the procedures are compared with similar
               techniques (e.g. codebook analysis) and strengths and weaknesses
               are discussed.",
  year      =  2014,
  url       = "https://nbn-resolving.org/urn:nbn:de:0168-ssoar-395173",
  keywords  = "Theory and Methods;Grounded Theory;Research Methods;\_New
               Literature",
  language  = "en"
}

@PHDTHESIS{Vermeer2019-xk,
  title    = "Collective Learning in Strategic Public Private Partnership
              ({PPP}) Procurement Systems for Social Infrastructure",
  author   = "Vermeer, Daniel Marinus Maria",
  abstract = "This thesis investigates how organizations in strategic Public
              Private Partnership (PPP) procurement systems—specifically through
              Local Education Partnerships (LEPs) in England—collectively learn
              to improve delivery of social infrastructure projects. It
              addresses a gap in understanding how collective learning occurs in
              complex, hybrid organizations formed by both public and private
              entities. Drawing from organizational learning and systems
              thinking, the study uses mixed methods to evaluate 12 LEPs and 600
              schools under the Building Schools for the Future (BSF) program.
              It introduces the Asset Value Enhancement Model (AVEM) as a
              framework for embedding collective learning throughout project
              lifecycles. The research highlights that sustained learning
              contributes significantly to achieving better whole-life value for
              money and environmental sustainability in infrastructure delivery.",
  year     =  2019,
  url      = "https://discovery.ucl.ac.uk/id/eprint/10086561/",
  keywords = "BSSC Thesis;\_BibTex to MD Git;Thesis;UCL",
  school   = "University College London"
