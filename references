               manipulatives; embodied visualizations;3rd Edition [2021] \_
               Multimedia Learning;Part VI - Principles Based on Social and
               Affective Features of Multimedia Learning;Multimedia
               [read];Multimedia}
}

@INBOOK{Wang2021-np,
  title     = {Multimedia Learning with Animated Pedagogical Agents},
  author    = {Wang, Fuxing and Li, Wenjing and Zhao, Tingting},
  editor    = {Fiorella, Logan and Mayer, Richard E},
