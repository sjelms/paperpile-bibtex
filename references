@ARTICLE{Terry2019-yh,
  title        = {Television as a career motivator and education tool: A
                  final-year nursing student cohort study},
  author       = {Terry, Daniel and Peck, Blake},
  journaltitle = {European Journal of Investigation in Health Psychology and
                  Education},
  publisher    = {MDPI AG},
  volume       = {10},
  issue        = {1},
  pages        = {346--357},
  date         = {2019-12-24},
  doi          = {10.3390/ejihpe10010026},
