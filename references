@ARTICLE{Georgiou2021-hp,
  title        = {Learning experience design with immersive virtual reality in
                  physics education},
  author       = {Georgiou, Yiannis and Tsivitanidou, Olia and Ioannou, Andri},
  journaltitle = {Educational technology research and development: ETR \& D},
  volume       = {69},
  issue        = {6},
