@ARTICLE{Basham2016-rl,
  title       = "An operationalized understanding of personalized learning",
  author      = "Basham, James D and Hall, Tracey E and Carter, Jr, Richard A
                 and Stahl, William M",
  affiliation = "University of Kansas, Lawrence, KS, USA; CAST, Wakefield, MA,
                 USA",
  journal     = "Journal of special education technology: a publication of Utah
