@INPROCEEDINGS{Kamaleeva2019-cr,
  title      = {Cognitive map of the pedagogical situation as a component of the
                pedagogical situation cognitive model (as exemplified by
                analyzing natural science and vocational training in colleges of
                the republic of tatarstan)},
  author     = {Kamaleeva, A R and Gilmanshina, S I and Nozdrina, N A},
