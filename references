@ARTICLE{Legg2007-tj,
  title    = "Universal Intelligence: A Definition of Machine Intelligence",
  author   = "Legg, Shane and Hutter, Marcus",
  journal  = "Minds and Machines",
  volume   =  17,
  number   =  4,
  pages    = "391--444",
  abstract = "A critical paper from two of the greatest thinkers in AI. Shane
              Legg is also one of the co-founders of Google DeepMind, where he
              continues his work in this area.",
