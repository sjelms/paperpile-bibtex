@ARTICLE{Hook2021-dq,
  title    = "Characterizing Interaction Design by Its Ideals: A Discipline in
              Transition",
  author   = "Höök, Kristina and Löwgren, Jonas",
  journal  = "She Ji: The Journal of Design, Economics, and Innovation",
  volume   =  7,
  number   =  1,
  pages    = "24--40",
  abstract = "As a way to capture a broadly acceptable high-level
              characterization of design, we focus on the guiding values or
              ideals of the discipline. We first reason from the notion of
              engineering interfaces for usability and utility up to the 1990s
              to the current ideal of designing interfaces for experience and
              meaning. Next, we identify three recent technical and societal
              developments that are challenging the existing ideals of
              interaction design, namely the move towards hybrid
              physical/digital materials, the emergence of an increasingly
              complex and fluid digital ecology, and the increasing proportion
              of autonomous or partially autonomous systems changing their
              behavior over time and with use. These challenges in turn motivate
              us to propose three directions in which new ideals for interaction
              design might be sought: the first is to go beyond the
              language-body divide that implicitly frames most of our current
              understandings of experience and meaning, the second is to extend
              the scope of interaction design from individual interfaces to the
              complex sociotechnical fabric of human and nonhuman actors, and
              the third is to go beyond predictability by learning to design
              with machine learning.",
  month    =  "1~" # mar,
  year     =  2021,
  url      = "https://www.sciencedirect.com/science/article/pii/S2405872621000010",
  keywords = "Interaction design; Ideals;Week 1\_ Design and technology;Journal
              Articles;Course Assignment;Economics",
  doi      = "10.1016/j.sheji.2020.12.001",
  issn     = "2405-8726"
}

