@ARTICLE{Ma2022-ld,
  title        = {Adaptable modular construction systems and multi-objective
                  optimisation strategies for mass-customised housing: A new
                  user-driven paradigm for high-rise living in Hong Kong},
  author       = {Ma, Chun Yu and van Ameijde, Jeroen},
  journaltitle = {International Journal of Architectural Computing},
  publisher    = {SAGE Publications},
  volume       = {20},
  issue        = {1},
  pages        = {96--113},
