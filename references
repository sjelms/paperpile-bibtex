@ARTICLE{Driscoll2011-fe,
  title        = {Writing for Publication: A Practical Six Step Approach},
  author       = {Driscoll, John and Aquilina, Reggie},
  journaltitle = {International Journal of Orthopaedic and Trauma Nursing},
  volume       = {15},
  issue        = {1},
  pages        = {41--48},
  date         = {2011-02-01},
  doi          = {10.1016/j.ijotn.2010.05.001},
  issn         = {1878-1241},
  abstract     = {There is a tendency amongst nurses in clinical practice to
                  view writing for publication as something beyond their
                  requisite. However, as UK nurses in particular move towards an
                  all graduate profession, the need for clinical nurses to
                  nurture a culture of writing is becoming more evident. Apart
                  from an opportunity to share best orthopaedic and trauma
                  nursing practice and innovation, writing for publication can
                  also provide personal satisfaction and professional growth.
                  The aim of this article is to inspire and encourage new
                  authors in clinical practice to write for publication. The
                  authors outline a practical Six Step Approach to act as
                  signposts towards creating that first publishable journal
                  article. This includes the need to read as preparation for
                  writing, selecting an appropriate journal, planning and
                  structuring the work as well as what happens after submission
                  of the publication. The article also includes practical tips
                  that will help the clinical practitioner to remain focused and
                  committed towards writing for publication.},
  url          = {https://www.sciencedirect.com/science/article/pii/S1878124110000468},
  keywords     = {Writing for publication; Six Step Approach to journal writing;
                  Overcoming barriers when writing an article; Practical writing
                  guide; The publishing process;PhD Support and Training}
}

