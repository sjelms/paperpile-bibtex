@ARTICLE{Alkhatlan2019-pv,
  title        = {Intelligent tutoring systems: A comprehensive historical
                  survey with recent developments},
  shorttitle   = {Intelligent Tutoring Systems},
  author       = {Alkhatlan, Ali and Kalita, Jugal},
  journaltitle = {International Journal of Computers \& Applications},
  publisher    = {Foundation of Computer Science},
  volume       = {181},
  issue        = {43},
