@ARTICLE{Levers2013-az,
  title       = "Philosophical paradigms, grounded theory, and perspectives on
                 emergence",
  author      = "Levers, Merry-Jo D",
  affiliation = "University of Calgary–Qatar, Doha, Qatar",
  journal     = "SAGE open",
  publisher   = "SAGE Publications",
  volume      =  3,
  number      =  4,
  pages       =  215824401351724,
  abstract    = "The purpose of this article is to present a plausible framework
                 to initiate discussion regarding the concept of emergence in
                 grounded theory. Using ontological and epistemological
                 positions to develop three research paradigms, and applying
                 these paradigms to various uptakes of grounded theory
                 demonstrates differences in the definition of emergence.
                 Discovery emergence is presented as a unidirectional
                 relationship between the constituent parts (data) and the
                 emergent property (theory) observed by an external
                 postpositivist grounded theorist. Mechanistic emergence is
                 presented as having a bidirectional relationship between the
                 constituent part (data) and the emergent property (theory)
                 observed by an external constructionist grounded theorist.
                 Finally, reflective emergence is presented as having a
                 bidirectional relationship between the constituent parts (data)
                 and the emergent property (theory) interpreted by an
                 interpretivist grounded theorist.",
  month       =  "1~" # jan,
  year        =  2013,
  url         = "http://dx.doi.org/10.1177/2158244013517243",
  keywords    = "Theory and Methods;Journal Articles;Research Methods",
  doi         = "10.1177/2158244013517243",
  issn        = "2158-2440",
  language    = "en"
}

@ARTICLE{Timonen2018-hr,
  title       = "Challenges when using grounded theory",
  author      = "Timonen, Virpi and Foley, Geraldine and Conlon, Catherine",
  affiliation = "School of Social Work and Social Policy, Trinity College
                 Dublin, Dublin, Ireland; School of Medicine, Trinity College
                 Dublin, Dublin, Ireland",
  journal     = "International journal of qualitative methods",
  publisher   = "SAGE Publications",
  volume      =  17,
  number      =  1,
  pages       =  160940691875808,
  abstract    = "The grounded theory (GT) method is widely applied, yet
                 frequently misunderstood. We outline the main variants of GT
                 and dispel the most common myths associated with GT. We argue
                 that the different variants of GT incorporate a core set of
                 shared procedures that can be put to work by any researcher or
                 team from their chosen ontological and epistemological
                 perspective. This “shared core” of the GT method is articulated
                 as the principles of (1) taking the word “grounded” seriously,
                 (2) capturing and explaining context-related social processes,
                 (3) pursuing theory through engagement with data, and (4)
                 pursuing theory through theoretical sampling. In this article,
                 we have put forward, in a nutshell, a distillation of core
                 principles underpinning existing GT approaches that can aid
                 further engagement with the different variants of GT. We are
                 motivated by the wish to make GT more comprehensible and
                 accessible, especially for researchers who are new to the
                 method.",
  month       =  "1~" # dec,
  year        =  2018,
  url         = "http://dx.doi.org/10.1177/1609406918758086",
  keywords    = "Grounded Theory;Journal Articles;Research Methods",
  doi         = "10.1177/1609406918758086",
  issn        = "1609-4069",
  language    = "en"
}

@ARTICLE{Singh2018-ur,
  title       = "Selecting a grounded theory approach for nursing research",
  author      = "Singh, Shaminder and Estefan, Andrew",
  affiliation = "University of Calgary, Calgary, Alberta, Canada.",
  journal     = "Global qualitative nursing research",
  publisher   = "SAGE Publications",
  volume      =  5,
  pages       =  2333393618799571,
  abstract    = "Grounded theory is a commonly used research methodology. There
                 are three primary approaches to grounded theory in nursing
                 research: those espoused by Glaser, Strauss and Corbin, and
                 Charmaz. All three approaches use similar procedures, yet there
                 are important differences among them, which implies that
                 researchers need to make careful choices when using grounded
                 theory. Researchers new to grounded theory need to find the
                 most appropriate approach that fits their research field,
                 topic, and researcher position. In this article, we compare the
                 three grounded theory approaches. Choices of a grounded theory
                 approach will depend on the researcher's understanding of the
                 philosophical underpinnings of all three approaches. Practical
                 aspects of grounded theory approaches should match the
                 information processing styles and analytical abilities of the
                 researcher and the intended use of the theory. We illustrate
                 key aspects of decision making about which method to select by
                 drawing upon the first author's experiences in his doctoral
                 research.",
  month       =  jan,
  year        =  2018,
  url         = "http://dx.doi.org/10.1177/2333393618799571",
  keywords    = "decision making; epistemology; grounded theory; method;
