@INCOLLECTION{Terzi2010-pr,
  title     = "The Capabilities Approach",
  booktitle = "International Encyclopedia of Education (Third Edition)",
  author    = "Terzi, L",
  editor    = "Peterson, Penelope and Baker, Eva and McGaw, Barry",
  abstract  = "Developed by Amartya Sen and Martha Nussbaum, the capabilities
               approach is a normative framework for the evaluation of human
               well-being and the justice of institutional arrangements.
               Central to this framework are people's capabilities to function,
               that is, their real freedoms (their capabilities) to be and to
               do what they value being and doing (their functionings). The
               approach provides fruitful insights for educational theory,
               policy, and practice. This article analyzes three fundamental
               dimensions suggested by the approach: the functions of
               education, the conception of justice and educational equality,
               and finally, the importance of pedagogy and curriculum for the
               development of people's overall freedom.",
