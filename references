@INBOOK{Aitchison2017-ps,
  title     = {Prefabricated Housing in Architectural Culture},
  author    = {Aitchison, Mathew and Macarthur, John},
  editor    = {Smith, Ryan E and Quale, John D},
  booktitle = {Offsite Architecture: Constructing the Future},
  publisher = {Routledge},
