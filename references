@ARTICLE{Antle2008-eh,
  title        = {Child-based personas: need, ability and experience},
  author       = {Antle, Alissa N},
  journaltitle = {Cognition, technology \& work},
  volume       = {10},
  issue        = {2},
  pages        = {155--166},
  date         = {2008-04-01},
  doi          = {10.1007/s10111-007-0071-2},
  issn         = {1435-5558,1435-5566},
  abstract     = {Interactive technologies are becoming ubiquitous in many
                  children’s lives. From school to home, technologies are
                  changing the way children live. However, current methods of
                  designing these technologies do not adequately consider
                  children’s needs and developmental abilities. This paper
                  describes and illustrates a new approach for creating user
                  abstractions of children called the child-persona technique.
                  Child-personas integrate theoretical concepts, empirically
                  generated data and experiential goals. An analysis of the
                  utility of this technique provides insights about how this
                  technique can benefit designers by generating realistic
                  child-user abstractions through a process which supports
                  designers in child-centric design.},
  url          = {https://doi.org/10.1007/s10111-007-0071-2},
  keywords     = {Week 6\_ Ideation and creative design;Journal
                  Articles;Optional;Course Assignment}
