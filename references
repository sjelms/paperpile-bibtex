  author     = {Dünser, Andreas and Steinbügl, Karin and Kaufmann, Hannes and
                Glück, Judith},
  booktitle  = {Proceedings of the 6th ACM SIGCHI New Zealand chapter's
                international conference on Computer-human interaction design
                centered HCI - CHINZ '06},
  publisher  = {Association for Computing Machinery},
  location   = {New York, New York, USA},
