@ONLINE{Science_Museum2018-ms,
  title     = {Goodbye to the hello girls: automating the telephone exchange},
  author    = {{Science Museum}},
  booktitle = {Science Museum},
  date      = {2018-10-22},
  abstract  = {The introduction of automatic telephone exchanges allowed callers
