@INBOOK{Leutner2021-no,
  title     = {The Drawing Principle in Multimedia Learning},
  author    = {Leutner, Detlev and Schmeck, Annett},
  editor    = {Fiorella, Logan and Mayer, Richard E},
  booktitle = {The Cambridge Handbook of Multimedia Learning},
  publisher = {Cambridge University Press},
  location  = {Cambridge},
  edition   = {3},
  pages     = {360--369},
  date      = {2021},
  doi       = {10.1017/9781108894333.038},
  isbn      = {9781108841580},
  abstract  = {This chapter discusses the drawing principle in multimedia
               learning. It proposes that asking students to create drawings
               while reading text causes generative processing that leads to
               better learning outcomes. In drawing, students have to translate
               the verbal text information into a picture that represents
               spatial relationships among functional elements referred to in
               the text. Asking students to draw a picture of the text content
               as they read encourages them to actively engage in deep cognitive
               and metacognitive processing and thus fosters deep understanding
               of the material to be learned. The drawing principle has been
               supported by many studies, especially when students engage in
               drawing using paper and pencil. An important logistical issue is
               to create a form of drawing activity that minimizes extraneous
               cognitive processing by providing appropriate support for
               drawing.},
  series    = {Cambridge Handbooks in Psychology},
  url       = {https://www.cambridge.org/core/books/cambridge-handbook-of-multimedia-learning/drawing-principle-in-multimedia-learning/E077437FA9CFEAFE3B1B037ADD2E75A1},
  keywords  = {drawing principle; generative processing; extraneous cognitive
               processing; generative theory of drawing construction; drawing
               support;3rd Edition [2021] \_ Multimedia Learning;Part VII -
               Principles Based on Generative Activity in Multimedia Learning}
}

@INBOOK{Renkl2021-dj,
  title     = {The Worked Example Principle in Multimedia Learning},
  author    = {Renkl, Alexander},
  editor    = {Fiorella, Logan and Mayer, Richard E},
